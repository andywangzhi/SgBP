    		$("#frm").submit();
		}
		function addOrUpdateRecord(){
			var year = $("#year").val();
			if(year == ""){
				alert("请选择年份");
				return;
			}
			var value = $("#rczyzl").val();
			if(value == ""){
				alert("请录入人才资源总量");
				return;
			}
			value = $("#rlzyzl").val();
			if(value == ""){
				alert("请录入人力资源总量");
				return;
			}
			value = $("#rlzyzl").val();
			if(value == ""){
				alert("请录入人力资源总量");
				return;
			}
			$("#frm").attr("action","<%=path%>/soa/rcgzkh!saveOrUpdateTJ.action?menuId=");
    		$("#frm").submit();
		}
		function checkData(obj){
			var value = $(obj).val();
			if(value == ""){
				return ;
			}
			if(!isFloat(value)){
				alert("请输入数字！");
				$(obj).attr("value","");
				return ;
			}		
		}
			
		function isFloat(str)   
		{   
		    if(/^(-?\d+)(\.\d+)?$/.test(str))   
		   {   
		        return true;   
		   }   
		    return false;   
		} 
