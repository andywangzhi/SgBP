.panel-body {
border-color:#BFBFBF;
border-right:1px solid #BFBFBF;
border-style:solid;
border-width:0 1px 1px;
overflow:auto;
}

.panel-header {
background:url("../images/bg.jpg") repeat-x scroll 0 0 transparent;
border:1px solid #BFBFBF;
color:#BFBFBF;
font-size:12px;
font-weight:bold;
line-height:15px;
padding:5px;
position:relative; text-align:left;
}
  
  
 .tabs-header{
	border:1px solid #BFBFBF;
	background:#ECECEC;
	border-bottom:0px;
	position:relative;
	overflow:hidden;
	padding:0px;
	padding-top:2px;
	overflow:hidden;
}

.tabs{
	list-style-type:none;
	height:26px;
	margin:0px;
	padding:0px;
	padding-left:4px;
	font-size:12px;
	width:5000px;
	border-bottom:1px solid #BFBFBF;
}

.tabs li{
	float:left;
	display:inline-block;
	margin1:0px 1px;
	margin-right:4px;
	margin-bottom:-1px;
	padding:0;
	position:relative;
	border:1px solid #BFBFBF;
	-moz-border-radius-topleft:5px;
	-moz-border-radius-topright:5px;
	-webkit-border-top-left-radius:5px;
	-webkit-border-top-right-radius:5px;
}

.tabs li.tabs-selected{
	border:1px solid #BFBFBF;
	border-bottom:1px solid #fff;
	border-top1:2px solid #8DB2E3;
}

.tabs-panels{
	margin:0px;
	padding:0px;
	border:0px solid #8DB2E3;
	border-top:0px;
	overflow:auto;
}