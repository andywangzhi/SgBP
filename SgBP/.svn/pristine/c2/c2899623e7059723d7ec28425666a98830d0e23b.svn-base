/* Reset样式重设*/
html,body,div,p,h1,h2,h3,h4,h5,h6,dl,dt,dd,ol,ul,li,a,span,img,table,caption,tbody,tfoot,thead,tr,th,td,form,iframe {margin:0;padding:0;border:0; }
ol, ul {list-style: none;}
img { vertical-align:-2px;}
input, textarea, select {margin:0;padding:0; vertical-align:1px}
button {vertical-align:middle}

/* body */
body {font: 12px SimSun,"Microsoft YaHei",SimHei,Verdana,sans-serif; color:#111; }
span, small { font-weight:normal; font-size:12px}


/* Links★★★★★ */
a {color: #333; text-decoration:none; cursor:pointer; }
a:hover {color: #f60; }

/* body */
body {background: #f3f6fb ;}

.login_bar {width:1100px; height:768px;position:absolute; left:50%;top:50%; margin-top:-384px; margin-left:-550px; background: url(../images/web_login_body_bg.jpg) center center no-repeat; }
.ul_input { padding-top:300px; padding-left:280px}
.ul_input li { padding-left:260px;margin-top:18px;overflow:hidden}
.ul_input span { font-size:14px; color:#fff; font-weight:bold}
.ul_input .dl_input { height:24px; line-height:20px; border:1px #ccc solid;text-indent:5px;}



.login_btn_ok {
	width:79px; height:34px; line-height:30px;margin:0; padding:0; border:0;
	font-size:14px; color:#f3f3f3; font-weight:bold;
	background: url(../images/web_login_btn_ok.gif) 0px 0; 
	vertical-align:middle;overflow:hidden;cursor:pointer;}
.login_btn_ok:hover {color:#fff; background: url(../images/web_login_btn_ok.gif) 79px 0; }

.login_btn_no {
	width:79px; height:34px; line-height:30px;margin:0; padding:0; border:0;
	font-size:14px; color:#f3f3f3; font-weight:bold;
	background: url(../images/web_login_btn_no.gif) 0px 0; 
	vertical-align:middle;overflow:hidden;cursor:pointer;}
.login_btn_no:hover {color:#fff; background: url(../images/web_login_btn_no.gif) 79px 0; }

.login_footer { position:absolute; bottom:0px; width:100%;height:26px;  font:normal 12px Verdana,sans-serif;color:#68cbfc;line-height:26px;text-align:center;}